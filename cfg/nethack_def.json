       [ "$hea", "%hum", "%gno", "%hlf", "%inc", "#neu" ],
       [ "$kni", "%hum", "%dwa", "%clk", "#law" ],
       [ "$mon", "%hum", "%clk", "%hlf", "%inc", "#law", "#neu", "#cha" ],
       [ "$nob", "%hum", "%dwa", "%elf", "%dro", "%vam", "%hlf", "%inc", "#law", "#neu", "#cha" ],
       [ "$pri", "%hum", "%elf", "%dro", "%clk", "%hlf", "%inc", "#law", "#neu", "#cha" ],
       [ "$pir", "%hum", "%elf", "%dro", "%hlf", "%inc", "#neu", "#cha" ],
       [ "$ran", "%hum", "%elf", "%dro", "%gno", "%orc", "%hlf", "%inc", "#neu", "#cha" ],
       [ "$rog", "%hum", "%orc", "%elf", "%dro", "%hlf", "%inc", "%vam", "#cha" ],
       [ "$sam", "%hum", "%clk", "#law" ],
       [ "$tou", "%hum", "%hlf", "%inc", "#neu" ],
       [ "$brd", "%hum", "%elf", "%dro", "%gno", "%hlf", "%inc", "#neu", "#cha" ],
       [ "$val", "%hum", "%dwa", "%hlf", "%inc", "#law", "#neu", "!fem" ],
       [ "$wiz", "%hum", "%elf", "%dro", "%gno", "%orc", "%vam", "%hlf", "%inc", "#neu", "#cha" ],

       [ "%elf", "#cha",         "#non" ],
       [ "%dwa", "#law",         "#non" ],
       [ "%gno", "#neu",         "#non" ],
       [ "%orc", "#cha",         "#non" ],
       [ "%vam", "#neu", "#cha", "#non" ],
       [ "%clk", "#law",         "#non" ],
       [ "%inc", "#neu", "#cha", "#non" ],

       [ [ "%dro", "!mal" ], "#neu", "#non" ],
       [ [ "%dro", "!fem" ], "#cha", "#non" ]

     ],

     "nhf" : [
       [ "$arc", "%hum", "%dwa", "%gno",         "#law", "#neu"         ],
       [ "$bar", "%hum", "%orc", "%dwa"                                 ],
       [ "$cav", "%hum", "%orc",                 "#neu", "#cha"         ],
       [ "$hea", "%hum", "%gno", "%syl",         "#neu"                 ],
       [ "$kni", "%hum",                         "#law"                 ],
       [ "$mon", "%hum"                                                 ],
       [ "$pri", "%hum", "%elf", "%dwa", "%syl"                         ],
       [ "$ran", "%hum", "%elf", "%gno", "%orc", "%syl", "#neu", "#cha" ],
       [ "$rog", "%hum", "%orc", "%elf", "%syl", "#cha"                 ],
       [ "$sam", "%hum",                         "#law"                 ],
       [ "$tou", "%hum", "%gno",                 "#neu"                 ],
       [ "$val", "%hum",                         "#neu",         "!fem" ],
       [ "$wiz", "%hum", "%elf", "%gno", "%orc", "%syl", "#neu", "#cha" ],

       [ "%elf", "#cha" ],
       [ "%dwa", "#law" ],
       [ "%gno", "#neu" ],
       [ "%orc", "#cha" ],
       [ "%syl", "#neu", "#cha" ]
     ]
