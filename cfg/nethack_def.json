       [ "$mon", "%hum", "%elf"                                         ],
       [ "$pri", "%hum", "%elf", "%gia"                                 ],
       [ "$ran", "%hum", "%elf", "%gno", "%orc", "%kob", "#neu", "#cha" ],
       [ "$rog", "%hum", "%orc", "%kob", "%ogr",         "#cha"         ],
       [ "$sam", "%hum",                                 "#law"         ],
       [ "$tou", "%hum",                                 "#neu"         ],
       [ "$val", "%hum", "%dwa", "%elf", "%gia",         "#law", "#neu", "!fem" ],
       [ "$wiz", "%hum", "%elf", "%gno", "%orc", "%kob", "%gia", "%ogr", "#neu", "#cha" ],

       [ "%dwa", "#law" ],
       [ "%gno", "#neu" ],
       [ "%orc", "#cha" ],
       [ "%kob", "#cha" ],
       [ "%ogr", "#cha" ]
     ],

     "dnh" : [
       [ "$arc", "%hum", "%dwa", "%gno", "%vam", "%clk", "%hlf", "%inc", "#law", "#neu" ],
