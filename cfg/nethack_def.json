       [ "$con", "%hum", "%dwa", "%gno", "%orc",         "%vam", "#cha"         ],
       [ "$hea", "%hum", "%gno",                         "#neu"                 ],
       [ "$kni", "%hum",                                 "#law"                 ],
       [ "$mon", "%hum"                                                         ],
       [ "$pri", "%hum", "%elf"                                                 ],
       [ "$ran", "%hum", "%elf", "%gno", "%orc",         "#neu", "#cha"         ],
       [ "$rog", "%hum", "%orc", "%vam",                 "#cha"                 ],
       [ "$sam", "%hum",                                 "#law"                 ],
       [ "$tou", "%hum",                                 "#neu"                 ],
       [ "$val", "%hum", "%dwa",                         "#law", "#neu", "!fem" ],
       [ "$wiz", "%hum", "%elf", "%gno", "%orc", "%vam", "#neu", "#cha"         ],

       [ "%elf", "#cha" ],
       [ "%dwa", "#law" ],
       [ "%gno", "#neu" ],
       [ "%orc", "#cha" ],
       [ "%vam", "#cha" ]
     ],

     "gh" : [
       [ "$arc", "%hum", "%dwa", "%gno",         "#law", "#neu"         ],
