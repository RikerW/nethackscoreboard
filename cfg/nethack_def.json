       [ "$pri"                         ],
       [ "$ran", "#neu", "#cha"         ],
       [ "$rog", "#neu", "#cha"         ],
       [ "$sam", "#law", "#neu"         ],
       [ "$tou", "#neu", "#cha"         ],
       [ "$val", "#law", "#neu", "!fem" ],
       [ "$wiz", "#neu", "#cha"         ],

       [ "%elf", "#neu", "#cha" ],
       [ "%dwa", "#neu", "#law" ],
       [ "%gno", "#neu", "#cha" ],
       [ "%orc", "#neu", "#cha" ]
     ],

     "unh" : [
       [ "$arc", "%hum", "%dwa", "%gno", "%vam",         "#law", "#neu"         ],
       [ "$bar", "%hum", "%orc", "%vam",                 "#neu", "#cha"         ],
