[%- INCLUDE header.tt title="Recent Games" -%]
[%- MACRO number(n) GET n.chunk(-3).join(',') -%]

<p id="varmenu">
[% FOREACH var IN variants %]
  [% IF var == 'all'; label = 'All'; ELSE; label = vardef.item(var); END %]
  [% IF variant == var %]
    <span class="selected">[% label %]</span>
  [% ELSE %]
    <a href="recent.[% var %].html"><span class="unselected">[% label %]</span></a>
  [% END %]
[% END %]
</p>

[% INCLUDE tab_recent.tt %]

[% INCLUDE footer.tt %]
